  <style type="text/css">

  </style>

  <![CDATA[

  <% 
    total = server.eval("@COUNT(scrum/sprint['status', 'active'].scrum/ticket)") * 1.0
    #context.write("<div>Total [%s%%]</div>" %(total))    
    CHART_WIDTH = 400
    BAR_HEIGHT = 15
    
    table = []
    table.append("<table>")

    statuses = server.eval("@SOBJECT(scrum/ticket_status)")
    for status in statuses:
        label = status.get("label")
        value = status.get("value")
        total_in_status = server.eval("@COUNT(scrum/sprint['status', 'active'].scrum/ticket['status', '%s'])" %value) * 1.0
        percent = (total_in_status / total) * 100
        width = (total_in_status / total) * CHART_WIDTH
        if width < 1:         
            width = 3 
       
        color = status.get("background")
        if not color:
            color = "#FFF"      


        table.append("<tr>")

        table.append("<td width='120'>")
        table.append("%s" %label)
        table.append("</td>")

        table.append("<td width='50'>")
        table.append("%2d%%" %percent)
        table.append("</td>")

        table.append("<td width='50'>")
        table.append("%s" %total_in_status)
        table.append("</td>")


        table.append("<td>")
        table.append("<div style='width:%s;height:%s;background-color:%s'><div/></div>" %(width, BAR_HEIGHT, color))
        table.append("</td>")
        table.append("</tr>")

    table.append("</table>")
    
    table = "".join(table)
    context.write(table)
    

  %>

  ]]>



 
    </html>
  </ticket_status_report_wdg>
